<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:HelloWorld.Resources.Bindings.Converters">
    
    <converter:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    <converter:LengthToVisibilityConverter x:Key="LengthToVisibilityConverter" />
    <converter:DictionaryConverter x:Key="DictionaryConverter" />
    <converter:NamedCollectionConverter x:Key="NamedCollectionConverter" />
    <converter:StringWidthConverter x:Key="StringWidthConverter" />
    
</ResourceDictionary>